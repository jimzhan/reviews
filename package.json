{
  "name": "reviews",
  "version": "0.1.0",
  "description": "react view components",
  "main": "package.json",
  "bin": {
    "eslint": "./node_modules/eslint/bin/eslint.js",
    "mocha": "./node_modules/mocha/bin/mocha",
    "webpack": "./node_modules/webpack/bin/webpack.js",
    "webpack-dev-server": "./node_modules/webpack-dev-server/webpack-dev-server.js"
  },
  "scripts": {
    "eslint": "npm run eslint",
    "prebuild": "npm run clean",
    "build": "webpack --progress --colors --config webpack.babel.js",
    "lint": "eslint src --ext=.js,.jsx",
    "clean": "rimraf build",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register --recursive src/**/*.test.js",
    "start": "webpack-dev-server --progress --inline --config webpack.babel.js",
    "watch": "webpack --progress --colors --watch --config webpack.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/reviews.git"
  },
  "keywords": [
    "react",
    "views",
    "boilerplate",
    "components"
  ],
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/reviews/issues"
  },
  "homepage": "https://github.com/jimzhan/reviews#readme",
  "devDependencies": {
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "latest",
    "css-loader": "^0.23.1",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-react": "latest",
    "react-hot-loader": "latest",
    "redux-devtools": "latest",
    "redux-devtools-dock-monitor": "latest",
    "redux-devtools-log-monitor": "latest",
    "redux-logger": "latest",
    "rimraf": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest",
    "webpack-merge": "latest"
  },
  "dependencies": {
    "antd": "^0.12.12",
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "enzyme": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "keymirror": "^0.1.1",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1"
  }
}
