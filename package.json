{
  "name": "reviews",
  "version": "0.1.0",
  "description": "react view components",
  "main": "package.json",
  "scripts": {
    "prebuild": "npm run lint && npm run clean",
    "build": "webpack --progress --colors --config webpack.babel.js",
    "lint": "node_modules/.bin/eslint src --ext=.jsx --ext=.js",
    "clean": "rm -r build",
    "pretest": "npm run lint",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register --recursive src/**/*.test.js",
    "start": "DEBUG=worker webpack-dev-server --config webpack.babel.js",
    "watch": "webpack --progress --colors --watch --plugin HotModuleReplacementPlugin --config webpack.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/reviews.git"
  },
  "keywords": [
    "react",
    "views",
    "components"
  ],
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/reviews/issues"
  },
  "homepage": "https://github.com/jimzhan/reviews#readme",
  "devDependencies": {
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-react": "latest",
    "react-hot-loader": "latest",
    "require-dir": "latest",
    "webpack": "latest",
    "webpack-dev-server": "latest",
    "webpack-merge": "latest"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "debug": "^2.2.0",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "superagent": "^1.8.0-beta.2"
  }
}
